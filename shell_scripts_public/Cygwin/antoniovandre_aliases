alias rss="nice -n 20 \"${DIRETORIOTRABALHO}${DIRETORIOCYGWIN}antoniovandre_leitorrss\""
alias uol="nice -n 20 \"${DIRETORIOTRABALHO}${DIRETORIOCYGWIN}antoniovandre_leitorrss http://rss.uol.com.br/feed/noticias.xml 120\""
