if [ "${2}" != "0" ]; then echo -ne "Carregando aliases... "; fi

alias ab="\"${DIRETORIOTRABALHO}${DIRETORIOCYGWIN}antoniovandre_background\""
alias f="\"${DIRETORIOTRABALHO}${DIRETORIOCYGWIN}antoniovandre_frases_aleatorias\""
