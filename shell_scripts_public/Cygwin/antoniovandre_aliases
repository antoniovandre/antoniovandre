alias apr="mintty.exe -e \"${DIRETORIOTRABALHO}${DIRETORIOCYGWIN}antoniovandre_anotacoesprivadas\"; while ! ls \"${DIRETORIOTRABALHO}${DIRETORIOCYGWIN}${ARQUIVOSENHAANOTACOESPRIVADAS}\" &>> /dev/null; do sleep 1s; done; source \"${DIRETORIOTRABALHO}${DIRETORIOCYGWIN}antoniovandre_anotacoesprivadas\" 1"

alias apdown="if echo \"Fazendo o download de ${ARQUIVOANOTACOES}...\"; dbxcli-windows-amd64.exe get \"/Legado ontológico/${ARQUIVOANOTACOES}\" \"F:\\\\${DIRETORIOTRABALHOP##*/}\\\\${ARQUIVOANOTACOES}\"; then echo -ne \"${AV_VERDE}Sucesso${AV_SEMCOR}. \"; else echo -e \"${AV_VERMELHO}Ocorreu um erro${AV_SEMCOR}. \"; fi; date | tr -d \"\\n\"; echo \".\""

alias aprdown="if echo \"Fazendo o download de ${ARQUIVOANOTACOESPRIVADAS}...\"; dbxcli-windows-amd64.exe get \"/Legado ontológico/${ARQUIVOANOTACOESPRIVADAS}\" \"F:\\\\${DIRETORIOTRABALHOP##*/}\\\\${ARQUIVOANOTACOESPRIVADAS}\"; then echo -ne \"${AV_VERDE}Sucesso${AV_SEMCOR}. \"; else echo -e \"${AV_VERMELHO}Ocorreu um erro${AV_SEMCOR}. \"; fi; date | tr -d \"\\n\"; echo \".\""

alias apbkdc="if echo \"Copiando ${ARQUIVOANOTACOES} para documentos no Dropbox...\"; dbxcli-windows-amd64.exe put \"F:\\\\${DIRETORIOTRABALHOP##*/}\\\\${ARQUIVOANOTACOES}\" \"/Documents/BACKUPED14/${ARQUIVOANOTACOES}\"; then echo -ne \"${AV_VERDE}Sucesso${AV_SEMCOR}. \"; else echo -e \"${AV_VERMELHO}Ocorreu um erro${AV_SEMCOR}. \"; fi; date | tr -d \"\\n\"; echo \".\""

alias apbklo="if echo \"Copiando ${ARQUIVOANOTACOES} para o Legado Ontológico...\"; dbxcli-windows-amd64.exe put \"F:\\\\${DIRETORIOTRABALHOP##*/}\\\\${ARQUIVOANOTACOES}\" \"/Legado ontológico/${ARQUIVOANOTACOES}\"; then echo -ne \"${AV_VERDE}Sucesso${AV_SEMCOR}. \"; else echo -ne \"${AV_VERMELHO}Ocorreu um erro${AV_SEMCOR}. \"; fi; date | tr -d \"\\n\"; echo \".\""

alias apbkc="if echo \"Copiando ${ARQUIVOANOTACOES} para a unidade C...\"; cp \"${DIRETORIOTRABALHO}${ARQUIVOANOTACOES}\" \"/cygdrive/c/Antonio Vandré Pedrosa Furtunato Gomes/${ARQUIVOANOTACOES}\" &>> /dev/null; then echo -ne \"${AV_VERDE}Sucesso${AV_SEMCOR}. \"; else echo -ne \"${AV_VERMELHO}Ocorreu um erro${AV_SEMCOR}. \"; fi; date | tr -d \"\\n\"; echo \".\""

alias apbkg="if echo \"Copiando ${ARQUIVOANOTACOES} para o diretório GitHub local...\"; cp \"${DIRETORIOTRABALHO}${ARQUIVOANOTACOES}\" \"${DIRETORIOTRABALHO}Mathematical Ramblings/GitHub/${ARQUIVOANOTACOES}\" &>> /dev/null; then echo -ne \"${AV_VERDE}Sucesso${AV_SEMCOR}. \"; else echo -ne \"${AV_VERMELHO}Ocorreu um erro${AV_SEMCOR}. \"; fi; date | tr -d \"\\n\"; echo \".\""

alias apbk="apbkc; apbkdc; apbklo; apbkg"

