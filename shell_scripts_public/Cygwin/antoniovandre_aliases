alias swlobk="if echo \"Copiando estatísticas do AV3DSpaceWar para o Legado ontológico...\"; dbxcli-windows-amd64.exe put \"F:\\\\${DIRETORIOTRABALHOP##*/}\\\\${ARQUIVOAV3DSPACEWARESTATISTICAS}\" \"/Legado ontológico/${ARQUIVOAV3DSPACEWARESTATISTICAS}\"; then echo -ne \"${AV_VERDE}Sucesso${AV_SEMCOR}. \"; else echo -e \"${AV_VERMELHO}Ocorreu um erro${AV_SEMCOR}. \"; fi; date | tr -d \"\\n\"; echo \".\""

alias swbk="swdcbk; swlobk"

# Início aliases temporários 2/2.

alias aliastemp="${DIRETORIOTRABALHO}${DIRETORIOCYGWIN}antoniovandre_temp.sh"

# Fim aliases temporários 2/2.

if [ "${2}" != "0" ]; then echo -e "${AV_VERDE}Ok${AV_SEMCOR}."; fi

