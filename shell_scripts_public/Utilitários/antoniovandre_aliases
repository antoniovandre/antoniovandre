alias Asc="antoniovandre_arcosecante"
alias Acs="antoniovandre_arcocossecante"
alias Pt="antoniovandre_potencia"
alias Rts="antoniovandre_reduzirtermossemelhantes"
alias Vnp="antoniovandre_valornumericopolinomio"
alias Ev="antoniovandre_eval"
alias Evn="antoniovandre_eval_nolog"
alias Der="antoniovandre_derivada"
alias Int="antoniovandre_integraldefinida"
alias Fmp="antoniovandre_funcaomaisproxima /home/antoniovandre/software_sources/C/Math/antoniovandre_pontos.txt /home/antoniovandre/software_sources/C/Math/antoniovandre_funcoes.txt 1"
alias Pc="antoniovandre_produtocomplexos"
alias Rz="antoniovandre_raizesfuncao"
alias Nr="antoniovandre_numeroraizes"
alias Dt="antoniovandre_determinante"

alias Pts="antoniovandre_proximotermosequencia"

alias Ce="if ls /home/antoniovandre/software_sources/C/Math/antoniovandre_math_estatisticas.txt &>> /dev/null; then cat /home/antoniovandre/software_sources/C/Math/antoniovandre_math_estatisticas.txt; else echo \"Arquivo de estatísticas matemáticas ainda não existente.\"; fi"

alias Re="rm /home/antoniovandre/software_sources/C/Math/antoniovandre_math_estatisticas.txt &>> /dev/null; rm /home/antoniovandre/software_sources/C/Math/antoniovandre_math_estatisticas.png &>> /dev/null"

alias Gp="antoniovandre_gnuplot_mathstatistics"

alias Comp="antoniovandre_cmath_compilar"

alias Grs="if ls /home/antoniovandre/software_sources/C/Math/antoniovandre_math_estatisticas.png &>> /dev/null; then eog /home/antoniovandre/software_sources/C/Math/antoniovandre_math_estatisticas.png &>> /dev/null; else echo 'Arquivo imagem gráfico não encontrado.'; fi"

alias Evg="python3 /home/antoniovandre/software_sources/Python/antoniovandre_eval_gui.py"

alias Gf="antoniovandre_funcoesgerador"

alias Gr="antoniovandre_graficocartesiano"

alias Grp="antoniovandre_graficocartesiano_parametrico"

alias Grpol="antoniovandre_graficocartesiano_polar"

alias Grppol="antoniovandre_graficocartesiano_parametricopolar"

alias Gr3="antoniovandre_graficocartesiano_3d"

alias Gr3sp="antoniovandre_graficocartesiano_3d_superficieparametrica"

alias Gr3spop="antoniovandre_graficocartesiano_3d_superficiepolar"

alias Gr3ct="antoniovandre_graficocartesiano_3d_curvatridimensional"

# Fim aliases matemáticos 1/1.

# Início aliases temporários 1/1.

alias aliastemp="evince /home/antoniovandre/livros\ \(antoniovandre-notebook\ specific\)/BACKUPED2/Cálculo\ I/cursoCalculoI-livro-a4-V2-2.pdf"

# Fim aliases temporários 1/1.

fi
